//u8_to_u16: U8 -> U16
u16 u8_to_u16 (u8 x) {
	return (u16)x;
}

//u8_to_u32: U8 -> U32
u32 u8_to_u32 (u8 x) {
	return (u32)x;
}

//u8_to_u64: U8 -> U64
u64 u8_to_u64 (u8 x) {
	return (u64)x;
}

//u16_to_u8: U16 -> U8
u8 u16_to_u8 (u16 x) {
	return (u8)x;
}

//u16_to_u32: U16 -> U32
u32 u16_to_u32 (u16 x) {
	return (u32)x;
}

//u32_to_u8: U32 -> U8
u8 u32_to_u8 (u32 x) {
	return (u8)x;
}

//u32_to_u16: U32 -> U16
u16 u32_to_u16 (u32 x) {
	return (u16)x;
}

//u32_to_u64: U32 -> U64
u64 u32_to_u64 (u32 x) {
	return (u64)x;
}

//u64_to_u32: U64 -> U32
u32 u64_to_u32 (u64 x) {
	return (u32)x;
}

u8 u64_to_u8(u64 x)
{
        return (u8)x;
}

