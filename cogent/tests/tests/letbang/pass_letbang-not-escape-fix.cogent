type A

type T = { f0 : U32, f1 : U8, f2 : A }

cmp : (A!, A!) -> Bool

foo :(T, A!) -> (T, U8)
foo (t, a) = 
  let ret = (
    let a' = t.f2 
     in if cmp (a, a') then 1 else 2) !t
  in (t,ret)
