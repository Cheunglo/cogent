type A
type R = {f1 : A, f2 : U8}

g : (U8, R) -> ()

foo : R -> ()
foo r = let f = \x => g (x,r)
         in f 3

bar : R -> (R take f1, A)
bar r = let f = \x => g (x,r)
        and r'{f1} = r
         in (r',f1)
